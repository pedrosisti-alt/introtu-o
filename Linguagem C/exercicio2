#include <stdio.h>
int main()
{
   int a=10,b=3;
   
   printf("Soma: %d\n",a+b);
   printf("Subtração: %d\n",a-b);
   printf("Multiplicação: %d\n",a*b);
   printf("Divisão: %d\n",a/b);
   printf("Módulo(resto da divisão): %d\n",a%b);
   
   float c=10.0, d=3.0;
   printf("Divisão: %.2f\n", c/d);
   return 0;
}
